Neo4j on conn "CREATE (:pokemon { poke_id : 1, poke_name : 'Bulbasaur', poke_type : 6,  descr : 'Bulbasaur can be seen napping in bright sunlight. There is a seed on its back. By soaking up the suns rays, the seed grows progressively larger.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/001.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 2, poke_name : 'Ivysaur', poke_type : 6, descr : 'There is a bud on this Pokemons back. To support its weight, Ivysaurs legs and trunk grow thick and strong. If it starts spending more time lying in the sunlight, its a sign that the bud will bloom into a large flower soon.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/002.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 3, poke_name : 'Venusaur', poke_type : 6,  descr : 'There is a large flower on Venusaurs back. The flower is said to take on vivid colors if it gets plenty of nutrition and sunlight. The flowers aroma soothes the emotions of people.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/003.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 4, poke_name : 'Charmander', poke_type : 2,  descr : 'The flame that burns at the tip of its tail is an indication of its emotions. The flame wavers when Charmander is enjoying itself. If the Pokemon becomes enraged, the flame burns fiercely.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/004.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 5, poke_name : 'Charmeleon', poke_type : 2, descr : 'Charmeleon mercilessly destroys its foes using its sharp claws. If it encounters a strong foe, it turns aggressive. In this excited state, the flame at the tip of its tail flares with a bluish white color.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/005.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 6, poke_name : 'Charizard', poke_type : 2,  descr : 'Charizard flies around the sky in search of powerful opponents. It breathes fire of such great heat that it melts anything. However, it never turns its fiery breath on any opponent weaker than itself.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/006.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 7, poke_name : 'Squirtle', poke_type : 4,  descr : 'Squirtles shell is not merely used for protection. The shells rounded shape and the grooves on its surface help minimize resistance in water, enabling this Pokemon to swim at high speeds.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/007.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 8, poke_name : 'Wartortle', poke_type : 4,descr : 'Its tail is large and covered with a rich, thick fur. The tail becomes increasingly deeper in color as Wartortle ages. The scratches on its shell are evidence of this Pokemons toughness as a battler.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/008.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 9, poke_name : 'Blastoise', poke_type : 4,  descr : 'Blastoise has water spouts that protrude from its shell. The water spouts are very accurate. They can shoot bullets of water with enough accuracy to strike empty cans from a distance of over 160 feet.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/009.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 10, poke_name : 'Caterpie', poke_type : 13, descr : 'Caterpie has a voracious appetite. It can devour leaves bigger than its body right before your eyes. From its antenna, this Pokemon releases a terrifically strong odor.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/010.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 11, poke_name : 'Metapod', poke_type : 13,  descr : 'The shell covering this Pokemons body is as hard as an iron slab. Metapod does not move very much. It stays still because it is preparing its soft innards for evolution inside the hard shell.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/011.png'})"

Neo4j on conn "CREATE (:pokemon { poke_id : 12, poke_name : 'Butterfree', poke_type : 13,  descr : 'Butterfree has a superior ability to search for delicious honey from flowers. It can even search out, extract, and carry honey from flowers that are blooming over six miles from its nest.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/012.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 13, poke_name : 'Weedle', poke_type : 13, descr : 'Weedle has an extremely acute sense of smell. It is capable of distinguishing its favorite kinds of leaves from those it dislikes just by sniffing with its big red proboscis (nose).', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/013.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 14, poke_name : 'Kakuna', poke_type : 13,  descr : 'Kakuna remains virtually immobile as it clings to a tree. However, on the inside, it is extremely busy as it prepares for its coming evolution. This is evident from how hot the shell becomes to the touch.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/014.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 15, poke_name : 'Beedrill', poke_type : 13,  descr : 'Beedrill is extremely territorial. No one should ever approach its nest—this is for their own safety. If angered, they will attack in a furious swarm.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/015.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 16, poke_name : 'Pidgey', poke_type : 5,  descr : 'Pidgey has an extremely sharp sense of direction. It is capable of unerringly returning home to its nest, however far it may be removed from its familiar surroundings.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/016.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 17, poke_name : 'Pidgeotto', poke_type : 5, descr : 'Pidgeotto claims a large area as its own territory. This Pokemon flies around, patrolling its living space. If its territory is violated, it shows no mercy in thoroughly punishing the foe with its sharp claws.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/017.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 18, poke_name : 'Pidgeot', poke_type : 5, descr : 'This Pokemon has a dazzling plumage of beautifully glossy feathers. Many Trainers are captivated by the striking beauty of the feathers on its head, compelling them to choose Pidgeot as their Pokemon.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/018.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 19, poke_name : 'Rattata', poke_type : 1, descr : 'Rattata is cautious in the extreme. Even while it is asleep, it constantly listens by moving its ears around. It is not picky about where it lives—it will make its nest anywhere.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/019.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 20, poke_name : 'Raticate', poke_type : 1,  descr : 'Raticates sturdy fangs grow steadily. To keep them ground down, it gnaws on rocks and logs. It may even chew on the walls of houses.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/020.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 21, poke_name : 'Spearow', poke_type : 5, descr : 'Spearow has a very loud cry that can be heard over half a mile away. If its high, keening cry is heard echoing all around, it is a sign that they are warning each other of danger.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/021.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 22, poke_name : 'Fearow', poke_type : 5,  descr : 'Fearow is recognized by its long neck and elongated beak. They are conveniently shaped for catching prey in soil or water. It deftly moves its long and skinny beak to pluck prey.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/022.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 23, poke_name : 'Ekans', poke_type : 7, descr : 'Ekans curls itself up in a spiral while it rests. Assuming this position allows it to quickly respond to a threat from any direction with a glare from its upraised head.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/023.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 24, poke_name : 'Arbok', poke_type : 7,  descr : 'This Pokemon is terrifically strong in order to constrict things with its body. It can even flatten steel oil drums. Once Arbok wraps its body around its foe, escaping its crunching embrace is impossible.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/024.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 25, poke_name : 'Pikachu', poke_type : 8,  descr : 'Whenever Pikachu comes across something new, it blasts it with a jolt of electricity. If you come across a blackened berry, its evidence that this Pokemon mistook the intensity of its charge.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/025.png'})"
Neo4j on conn "CREATE (:pokemon { poke_id : 26, poke_name : 'Raichu', poke_type : 8,  descr : 'If the electrical sacs become excessively charged, Raichu plants its tail in the ground and discharges. Scorched patches of ground will be found near this Pokemons nest.', image : 'http://assets22.pokemon.com/assets/cms2/img/pokedex/full/026.png'})"

